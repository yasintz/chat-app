{
  "name": "authenticator",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/authenticator/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "generatePackageJson": true,
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/apps/authenticator",
        "main": "apps/authenticator/src/main.ts",
        "tsConfig": "apps/authenticator/tsconfig.app.json",
        "assets": ["apps/authenticator/src/assets"]
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "fileReplacements": [
            {
              "replace": "apps/authenticator/src/environments/environment.ts",
              "with": "apps/authenticator/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nrwl/js:node",
      "options": {
        "buildTarget": "authenticator:build"
      },
      "configurations": {
        "production": {
          "buildTarget": "authenticator:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/authenticator/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/authenticator/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "dumpdb": {
      "executor": "nx:run-commands",
      "options": {
        "command": "sqlite3 dist/db.sqlite .dump > apps/authenticator/src/database/dump/data.sql"
      }
    },
    "loaddb": {
      "executor": "nx:run-commands",
      "options": {
        "command": "sqlite3 dist/db.sqlite < apps/authenticator/src/database/dump/data.sql"
      }
    },
    "type-check": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc -p apps/chat/tsconfig.app.json --noEmit"
      }
    }
  },
  "tags": []
}
