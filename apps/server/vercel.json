{
  "version": 2,
  "buildCommand": "nx vercel-build server",
  "outputDirectory": ".",
  "installCommand": "pnpm install",
  "builds": [
    {
      "src": "main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.js",
      "methods": ["GET", "POST", "PUT", "DELETE"]
    }
  ]
}
