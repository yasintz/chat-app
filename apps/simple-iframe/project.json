{
  "name": "simple-iframe",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/simple-iframe/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/vite:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "compiler": "babel",
        "outputPath": "dist/apps/simple-iframe",
        "index": "apps/simple-iframe/src/index.html",
        "baseHref": "/",
        "main": "apps/simple-iframe/src/main.tsx",
        "polyfills": "apps/simple-iframe/src/polyfills.ts",
        "tsConfig": "apps/simple-iframe/tsconfig.app.json",
        "assets": [
          "apps/simple-iframe/src/favicon.ico",
          "apps/simple-iframe/src/assets"
        ],
        "styles": [],
        "scripts": [],
        "webpackConfig": "@nrwl/react/plugins/webpack"
      },
      "configurations": {
        "development": {
          "sourceMap": true
        },
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/simple-iframe/src/environments/environment.ts",
              "with": "apps/simple-iframe/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nrwl/vite:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "simple-iframe:build",
        "hmr": true,
        "port": 4201
      },
      "configurations": {
        "development": {
          "buildTarget": "simple-iframe:build:development"
        },
        "production": {
          "buildTarget": "simple-iframe:build:production",
          "hmr": false
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/simple-iframe/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/simple-iframe/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "type-check": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc -p apps/simple-iframe/tsconfig.app.json --noEmit"
      }
    }
  },
  "tags": []
}
