{
  "name": "helpers-client",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/helpers/client/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/helpers/client",
        "main": "libs/helpers/client/src/index.ts",
        "tsConfig": "libs/helpers/client/tsconfig.lib.json",
        "assets": ["libs/helpers/client/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/helpers/client/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/helpers/client/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
